{
  "name": "feedback-tool",
  "version": "0.1.0",
  "scripts": {
    "firebase": "node_modules/firebase-tools/lib/bin/firebase.js",
    "build-dev": "./node_modules/.bin/esbuild compiled/app.js --bundle --define:process.env.NODE_ENV=\\\"development\\\" --outfile=public/app.js",
    "build-prod": "./node_modules/.bin/esbuild compiled/app.js --bundle --define:process.env.NODE_ENV=\\\"production\\\" --outfile=public/app.js",
    "compile": "node_modules/.bin/tsc src/* --jsx react --outDir compiled",
    "compile-and-build": "npm run compile; npm run build-dev",
    "watch": "fswatch -0 ./src/* | xargs -0 -n 1 -I {} npm run compile-and-build",
    "develop": "npm run compile-and-build; npm run watch"
  },
  "devDependencies": {
    "esbuild": "^0.8.17",
    "firebase-tools": "^8.16.2",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "firebase": "^8.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-redux-firebase": "^3.8.1",
    "redux": "^4.0.5"
  }
}
